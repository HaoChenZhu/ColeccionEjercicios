#include <iostream>
#include <array>
#include <vector>
using namespace std;
int main() {
  array<int,6>arr{1,3,4,5,6,8};
  vector<int>vectorcito;
  int i=0;
  while(i<arr.size()){
    if(arr[i]%2==0){
      vectorcito.push_back(arr[i]);
    }
    i++;
  }
  for(auto e:vectorcito){
    cout<<e<<" ";
  }
}
