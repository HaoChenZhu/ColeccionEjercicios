#include <iostream>
using namespace std;
int main() {
  string sentence{"Tres tristes tigres comen trigo en un trigal"};
  for(int i=0; i<=sentence.length(); i++){
      if (sentence.find("tri", i) != -1) {
			std::cout << sentence.find("tri", i) << "-";
			i = sentence.find("tri", i);
  }
  
  }
}
